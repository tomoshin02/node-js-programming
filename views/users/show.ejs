<h2>User Data: <%=user.fullName %></h2>
  <table class="table">
    <tr>
      <th>Name</th>
      <td><%= user.fullName %></td>
    </tr>
    <tr>
      <th>Email</th>
      <td><%= user.Email %></td>
    </tr>  
    <tr>
      <th>Zip Code</th>
      <td><%= user.zipCode %></td>
    </tr>
    <tr>
      <th>Password</th>
      <td><%= user.password %></td>
    </tr>  
  </table>

  <%if (user.subscribedAccount) {%>
  <h4>
    This user has a 
    <a href="<%=`/subscribers/${user.subscribedAccount}`%>">
    subscribed account</a>.
    <%}%>
  </h4>
